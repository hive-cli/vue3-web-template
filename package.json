{
  "name": "vue3-web-template",
  "private": true,
  "version": "1.0.0",
  "lint-staged": {
    "src/**/*.{js,jsx,vue}": "npm run lint:script",
    "src/**/*.{css,scss,vue}": "npm run lint:style"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint:script": "eslint --ext .js,.jsx,.vue --fix --quiet ./",
    "lint:style": "stylelint --fix \"src/**/*.{css,scss,vue}\"",
    "prepare": "husky install",
    "commit": "git-cz",
    "lint-staged": "lint-staged"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "echarts": "^5.4.3",
    "pinia": "^2.1.6",
    "qs": "^6.11.2",
    "ua-parser-js": "^1.0.35",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@iconify-json/mdi": "^1.1.54",
    "@iconify/vue": "^4.1.1",
    "@vitejs/plugin-vue": "^4.3.2",
    "@vueuse/core": "^10.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "naive-ui": "^2.34.4",
    "postcss-html": "^1.5.0",
    "prettier": "^3.1.0",
    "sass": "^1.66.1",
    "stylelint": "^15.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-recommended-scss": "^13.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.1.0",
    "unplugin-icons": "^0.16.5",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-define-options": "^1.3.15",
    "vite": "^4.4.9",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-windicss": "^1.9.1",
    "windicss": "^3.5.6"
  }
}
